[CREATE TYPE KIND IDENTIFIER NUMBER]
[STRUCTURE IF, (IDENTIFIER LESS NUMBER)
    [WRITE, STRING]
    [WRITE, STRING]
    [STRUCTURE IF (IDENTIFIER EQUALS NUMBER)
        [WRITE STRING]
    ]
    [STRUCTURE WHILE (IDENTIFIER LESS NUMBER)
        [STRUCTURE IF, IDENTIFIER EQUALS NUMBER
            [WRITE STRING]
        ]
    ]
    [STRUCTURE DO-WHILE (IDENTIFIER EQUAL NUMBER)
        [WRITE STRING]
        [STRUCTURE IF (IDENTIFIER EQUAL NUMBER)
            [WRITE STRING]
            [SET IDENTIFIER TO NUMBER]
        ]
    ]
]
